{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "MLMappingDocument": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$MLMappingDocument"
        },
        "mlModelFileName": {
          "type": "string"
        },
        "mlModelMetadata": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MLModelMetadata"
          }
        },
        "mlModelMappings": {
          "$ref": "#/definitions/MLModelMappings"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "MLModelEntityMappings": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$MLModelEntityMappings"
        },
        "entity": {
          "type": "string",
          "description": "domainmodels.schema.json#/definitions/IEntity"
        },
        "mappings": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TensorMappingElement"
          }
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "MLModelMappings": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$MLModelMappings"
        },
        "input": {
          "$ref": "#/definitions/MLModelEntityMappings"
        },
        "output": {
          "$ref": "#/definitions/MLModelEntityMappings"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "MLModelMetadata": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$MLModelMetadata"
        },
        "name": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "TensorDimension": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$TensorDimension"
        },
        "dimension": {
          "type": "string"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "TensorMappingElement": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "MLMappings$TensorMappingElement"
        },
        "tensorName": {
          "type": "string"
        },
        "tensorType": {
          "type": "string",
          "enum": [
            "UnsignedInteger8Tensor",
            "UnsignedInteger16Tensor",
            "Float16Tensor",
            "Complex64Tensor",
            "Complex128Tensor",
            "Integer8Tensor",
            "Integer16Tensor",
            "Integer32Tensor",
            "Integer64Tensor",
            "Float32Tensor",
            "Float64Tensor",
            "StringTensor",
            "BooleanTensor",
            "UnknownTensor"
          ]
        },
        "tensorShape": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TensorDimension"
          }
        },
        "staticTensorShape": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TensorDimension"
          }
        },
        "attribute": {
          "type": "string",
          "description": "domainmodels.schema.json#/definitions/IAttribute"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    }
  }
}