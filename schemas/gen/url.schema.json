{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "UrlSegment": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "Url$UrlSegment"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "StaticUrlSegment": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "Url$StaticUrlSegment"
        },
        "segment": {
          "type": "string",
          "default": ""
        }
      },
      "required": [
        "$ID",
        "$Type"
      ],
      "allOf": [
        {
          "$ref": "#/definitions/UrlSegment"
        }
      ]
    }
  }
}