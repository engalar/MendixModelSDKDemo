{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "CustomIcon": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "CustomIcons$CustomIcon"
        },
        "name": {
          "type": "string"
        },
        "characterCode": {
          "type": "string"
        },
        "tags": {
          "type": "string"
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    },
    "CustomIconCollection": {
      "type": "object",
      "properties": {
        "$ID": {
          "$ref": "../common.schema.json#/definitions/Identifiable"
        },
        "$Type": {
          "type": "string",
          "const": "CustomIcons$CustomIconCollection"
        },
        "collectionClass": {
          "type": "string"
        },
        "prefix": {
          "type": "string"
        },
        "fontData": {
          "type": "string"
        },
        "icons": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/CustomIcon"
          }
        }
      },
      "required": [
        "$ID",
        "$Type"
      ]
    }
  }
}